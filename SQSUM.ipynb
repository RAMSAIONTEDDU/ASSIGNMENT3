    "class Point:\n",
    "    def __init__(self,x,y,z):\n",
    "        self.x=x\n",
    "        self.y=y\n",
    "        self.z=z\n",
    "    def sqsum(self):\n",
    "        return ((self.x**2)+(self.y**2)+(self.z**2))\n",
    "x=int(input(\"X: \"))\n",
    "y=int(input(\"y: \"))\n",
    "z=int(input(\"z: \"))\n",
    "p1=Point(x,y,z)\n",
    "print(p1.sqsum())\n",
    "    "
