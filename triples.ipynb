   "source": [
    "def triple_num(n):\n",
    "    return n+n+n\n",
    "lst = []\n",
    "n =int(input(\"Enter number of elements in a List: \"))\n",
    "for i in range(0,n):\n",
    "    ele = int(input(\"enter ele: \"))\n",
    "    lst.append(ele)\n",
    "r = map(triple_num,lst)\n",
    "print(\"TRIPLE OF LIST ELEMNTS:\")\n",
    "print(list(r))"
   ]
